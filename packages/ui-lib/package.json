{
  "name": "ui-lib",
  "version": "0.0.1",
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "scripts": {
    "start": "npm run styleguidist",
    "prebuild": "rimraf build",
    "build:w": "npm run build:source -- --w",
    "build": "tsc",
