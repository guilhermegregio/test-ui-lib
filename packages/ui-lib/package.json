  "files": [
    "build/lib"
  ],
  "dependencies": {
    "emotion": "^9.2.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.2.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-react": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@types/node": "^9.6.5",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "@types/react-test-renderer": "^16.0.1",
    "react-docgen-typescript": "^1.3.0",
    "react-styleguidist": "^7.0.6",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.2",
    "typescript": "^2.8.1"
